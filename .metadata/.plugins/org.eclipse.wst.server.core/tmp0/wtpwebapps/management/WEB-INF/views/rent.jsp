<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

    <table border="1">
        <thead>
            <tr>
				<th>cust_no</th>
				<th>rent_no</th>
				<th>book_code</th>
				<th>rent_price</th>
				<th>rent_date</th>
            </tr>
        </thead>
        <tbody>
            <c:forEach items="${rentList}" var="item" varStatus="loop">
                <tr>
                    <td>${loop.index + 1}</td>
                    <td>${item.rent_no}</td>
                    <td>${item}</td>
                    <td>${item}</td>
                    <td>${item}</td>
                </tr>
            </c:forEach>
        </tbody>
    </table>
    
</body>
</html>